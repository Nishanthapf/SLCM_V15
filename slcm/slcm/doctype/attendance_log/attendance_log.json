{
  "doctype": "DocType",
  "name": "Attendance Log",
  "module": "SLCM",

  "autoname": "LOG-.YYYY.-.#####",
  "naming_rule": "Expression (old style)",
  "allow_rename": 1,

  "field_order": [
    "rfid_uid",
    "student",
    "swipe_time",
    "device_id",
    "location",
    "source",
    "processed",
    "student_attendance"
  ],

  "fields": [
    {
      "fieldname": "rfid_uid",
      "fieldtype": "Data",
      "label": "RFID UID",
      "reqd": 1,
      "in_list_view": 1
    },
    {
      "fieldname": "student",
      "fieldtype": "Link",
      "label": "Student",
      "options": "Student Master",
      "in_list_view": 1
    },
    {
      "fieldname": "swipe_time",
      "fieldtype": "Datetime",
      "label": "Swipe Time",
      "reqd": 1,
      "in_list_view": 1
    },
    {
      "fieldname": "device_id",
      "fieldtype": "Data",
      "label": "Device ID"
    },
    {
      "fieldname": "location",
      "fieldtype": "Data",
      "label": "Location"
    },
    {
      "fieldname": "source",
      "fieldtype": "Select",
      "label": "Source",
      "options": "RFID\nManual\nQR",
      "default": "RFID",
      "reqd": 1
    },
    {
      "fieldname": "processed",
      "fieldtype": "Check",
      "label": "Processed",
      "default": 0,
      "in_list_view": 1
    },
    {
      "fieldname": "student_attendance",
      "fieldtype": "Link",
      "label": "Student Attendance",
      "options": "Student Attendance"
    }
  ],

  "permissions": [
    {
      "role": "System Manager",
      "read": 1,
      "write": 1,
      "create": 1,
      "delete": 0,
      "print": 1,
      "email": 1,
      "export": 1,
      "report": 1,
      "share": 1
    }
  ],

  "row_format": "Dynamic",
  "sort_field": "modified",
  "sort_order": "DESC"
}
